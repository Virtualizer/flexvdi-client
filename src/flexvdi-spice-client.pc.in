prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: flexvdi-spice-client
Description: flexVDI extensions plugin for the SPICE client
Version: @FLEXVDI_VERSION@

Requires: @REQUIRED_PKGS@
Libs: -L${libdir} -lflexvdi-spice-client @EXTRA_PKGS_LDFLAGS@
Cflags: -I${includedir} @EXTRA_PKGS_CFLAGS@
