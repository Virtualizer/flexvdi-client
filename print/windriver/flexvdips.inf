DataSection=PSCRIPT_DATA    ; PSCRIPT Data Section
DataFile=flexvdips.ppd
Include=NTPRINT.INF         ; Include NTPRINT.INF.
Needs=PSCRIPT.OEM           ; Install PSCRIPT.

