[DestinationDirs]
DefaultDestDir=66000 ; DIRID_PRINTER_DRIVERS

[Manufacturer]
%Provider%=flexVDI, NTamd64

[flexVDI]
"flexVDI Printer" = flexvdips

[flexVDI.NTamd64]
